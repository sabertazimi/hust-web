{
  "name": "meact",
  "version": "2.6.0",
  "description": "Minimal react-like library",
  "main": "./src/index.js",
  "scripts": {
    "build": "webpack --mode production && yarn build:post",
    "build:post": "mkdir -p ../../build/react/meact && mv ./build/* ../../build/react/meact",
    "dev": "webpack serve --port 2333 --mode development --open",
    "start": "yarn dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/meact.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "react"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/meact/issues"
  },
  "homepage": "https://github.com/sabertazimi/meact#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-meact-jsx": "^0.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "html-loader": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.3",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  }
