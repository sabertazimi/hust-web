{
  "name": "gba",
  "version": "2.1.0",
  "description": "Minimal webpack boilerplate",
  "main": "./src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production && yarn build:post",
    "build:post": "mkdir -p ../../build/react/gba && mv ./build/* ../../build/react/gba",
    "dev": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "lint": "stylelint ./src/**/*.scss && eslint --fix --ext .js --ext .jsx ./src",
    "start": "yarn dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/gba.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "webpack",
    "boilerplate"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/gba/issues"
  },
  "homepage": "https://github.com/sabertazimi/gba",
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/eslint-parser": "7.15.4",
    "@babel/plugin-proposal-object-rest-spread": "7.15.6",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-react": "7.14.5",
    "autoprefixer": "10.3.4",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.2.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.2",
    "mini-css-extract-plugin": "2.3.0",
    "node-sass": "6.0.1",
    "postcss": "8.3.6",
    "postcss-loader": "6.1.1",
    "prettier": "2.4.0",
    "sass-loader": "12.1.0",
    "style-loader": "3.2.1",
    "stylelint": "13.13.1",
    "stylelint-config-mass": "5.3.8",
    "stylelint-webpack-plugin": "3.0.1",
    "webpack": "5.52.1",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.2.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
