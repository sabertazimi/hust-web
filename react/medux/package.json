{
  "name": "medux",
  "version": "2.1.0",
  "description": "Minimal redux-like library",
  "main": "index.js",
  "engines": {
    "node": ">=9"
  },
  "scripts": {
    "build": "webpack --mode production && yarn build:post",
    "build:post": "mkdir -p ../../build/react/medux && mv ./build/* ../../build/react/medux",
    "dev": "webpack serve --port 2333 --mode development --open",
    "start": "yarn dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/medux.git"
  },
  "keywords": [
    "redux",
    "react"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/medux/issues"
  },
  "homepage": "https://github.com/sabertazimi/medux",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.2",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "style-loader": "^3.2.1",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
