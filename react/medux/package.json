{
  "name": "medux",
  "version": "2.6.0",
  "description": "Minimal redux-like library",
  "main": "index.js",
  "engines": {
    "node": ">=9"
  },
  "scripts": {
    "build": "webpack --mode production && yarn build:post",
    "build:post": "mkdir -p ../../build/react/medux && mv ./build/* ../../build/react/medux",
    "dev": "webpack serve --port 2333 --mode development --open",
    "start": "yarn dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/medux.git"
  },
  "keywords": [
    "redux",
    "react"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/medux/issues"
  },
  "homepage": "https://github.com/sabertazimi/medux",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
