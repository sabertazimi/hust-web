{
  "name": "hust-web",
  "private": "true",
  "description": "A repo for learning demo archives",
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna clean --yes",
    "lint": "lerna run lint",
    "postbuild": "cp -fr index.html index.css ./build",
    "prebuild": "rm -fr ./build && mkdir -p ./build",
    "release": "node scripts/release.js",
    "start": "npm start -w mvvm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/hust-web.git"
  },
  "keywords": [
    "html",
    "html5",
    "css",
    "css3",
    "js",
    "javascript",
    "ecmascript",
    "react",
    "reactjs",
    "node",
    "nodejs",
    "front-end",
    "front-end development",
    "web",
    "web development"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/hust-web/issues"
  },
  "homepage": "https://github.com/sabertazimi/hust-web",
  "devDependencies": {
    "chalk": "^4.1.2",
    "consola": "^2.15.3",
    "lerna": "^4.0.0",
    "semver": "^7.3.5"
  },
  "workspaces": [
    "./css",
    "./css/bootstrap-cms",
    "./fcc",
    "./ife/2016",
    "./ife/2018",
    "./ife/2018/basic/mis",
    "./ife/2018/basic/oop",
    "./ife/2018/mvvm",
    "./js",
    "./react",
    "./react/*"
  ]
}
