{
  "name": "mis",
  "version": "2.7.0",
  "description": "Management Information System",
  "main": "./src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production && yarn build:post",
    "build:post": "mkdir -p ../../../../build/ife/2018/basic/mis && mv ./build/* ../../../../build/ife/2018/basic/mis",
    "dev": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "lint": "stylelint ./src/**/*.{css,scss} && eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "stylelint ./src/**/*.{css,scss} --fix && eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "start": "yarn dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/hust-web.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "mis"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/hust-web/issues"
  },
  "homepage": "https://github.com/sabertazimi/hust-web",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.9",
    "@dg-scripts/eslint-config": "^5.14.2",
    "@dg-scripts/stylelint-config": "^5.14.2",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.9.1",
    "eslint": "^8.56.0",
    "eslint-webpack-plugin": "^4.0.1",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.7",
    "postcss": "^8.4.33",
    "postcss-loader": "^8.0.0",
    "prettier": "^3.2.4",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.0",
    "stylelint-webpack-plugin": "^4.1.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
