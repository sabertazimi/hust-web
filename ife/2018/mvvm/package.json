    "san-loader": "^0.3.1",
    "style-loader": "^3.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-mass": "^5.3.12",
    "stylelint-webpack-plugin": "^3.0.1",
    "typescript": "^4.4.3",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
