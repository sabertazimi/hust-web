{
  "name": "mvvm",
  "version": "2.0.1",
  "description": "IFE 2018 MVVM Project",
  "main": "./src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production && yarn build:post",
    "build:post": "mkdir -p ../../../build/ife/2018/mvvm && mv ./build/* ../../../build/ife/2018/mvvm",
    "dev": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "lint": "stylelint ./src/**/*.css && eslint --ext .js ./src",
    "start": "yarn dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/hust-web.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "mvvm",
    "san",
    "webpack"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/hust-web/issues"
  },
  "homepage": "https://sabertazimi.github.io/hust-web/ife/2018/mvvm",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/preset-env": "^7.15.8",
    "@dg-scripts/eslint-config": "^1.1.3",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "eslint": "^7.32.0",
