{
  "name": "mvvm",
  "version": "2.7.0",
  "description": "IFE 2018 MVVM Project",
  "main": "./src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production && yarn build:post",
    "build:post": "mkdir -p ../../../build/ife/2018/mvvm && mv ./build/* ../../../build/ife/2018/mvvm",
    "dev": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "lint": "stylelint ./src/**/*.{css,scss} && eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "stylelint ./src/**/*.{css,scss} --fix && eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "start": "yarn dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/hust-web.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "mvvm",
    "san",
    "webpack"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/hust-web/issues"
  },
  "homepage": "https://sabertazimi.github.io/hust-web/ife/2018/mvvm",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@dg-scripts/eslint-config": "^5.11.0",
    "@dg-scripts/stylelint-config": "^5.11.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "eslint": "^8.28.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.1",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.0",
    "san-loader": "^0.3.5",
    "style-loader": "^3.3.1",
    "stylelint": "^14.15.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "async-validator": "^4.2.5",
    "san": "^3.12.1",
    "san-router": "^2.0.1"
  }
}
