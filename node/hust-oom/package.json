    "express": "^4.17.1",
    "express-rate-limit": "^5.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0"
