{
  "name": "chat",
  "version": "2.9.0",
  "description": "Chat System",
  "author": "sabertazimi",
  "license": "MIT",
  "homepage": "https://github.com/sabertazimi/hust-oom",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/hust-oom.git"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/hust-oom/issues"
  },
  "keywords": [
    "chat",
    "react",
    "webpack"
  ],
  "main": "./client/index.js",
  "engines": {
    "node": ">=9"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "client": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "lint": "stylelint ./client/**/*.{css,scss} && eslint ./client ./server",
    "lint:fix": "stylelint ./client/**/*.{css,scss} --fix && eslint ./client ./server --fix",
    "server": "nodemon ./server/index.js",
    "start": "concurrently \"pnpm:server\" \"pnpm:client\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@livechat/ui-kit": "^0.5.0-35",
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.8.2",
    "redux": "^5.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-object-rest-spread": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@dg-scripts/eslint-config": "^5.21.6",
    "@dg-scripts/stylelint-config": "^5.21.6",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.35.0",
    "eslint-webpack-plugin": "^5.0.2",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.4",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.6.2",
    "sass": "^1.92.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "stylelint": "^16.23.1",
    "stylelint-webpack-plugin": "^5.0.1",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
