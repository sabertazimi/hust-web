{
  "name": "chat",
  "version": "2.7.0",
  "description": "Chat System",
  "main": "./client/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "client": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "lint": "stylelint ./client/**/*.{css,scss} && eslint --ext .js,.jsx,.ts,.tsx ./client ./server",
    "lint:fix": "stylelint ./client/**/*.{css,scss} --fix && eslint --ext .js,.jsx,.ts,.tsx ./client ./server --fix",
    "server": "nodemon ./server/index.js",
    "start": "concurrently \"yarn:server\" \"yarn:client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/hust-oom.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "chat",
    "react",
    "webpack"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/hust-oom/issues"
  },
  "homepage": "https://github.com/sabertazimi/hust-oom",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@dg-scripts/eslint-config": "^5.14.0",
    "@dg-scripts/stylelint-config": "^5.14.0",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.49.0",
    "eslint-webpack-plugin": "^4.0.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.29",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.3",
    "stylelint-webpack-plugin": "^4.1.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@livechat/ui-kit": "^0.5.0-27",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.16.0",
    "redux": "^4.2.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2"
  }
}
