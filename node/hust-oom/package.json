{
  "name": "chat",
  "version": "2.7.0",
  "description": "Chat System",
  "main": "./client/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "client": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "lint": "stylelint ./client/**/*.{css,scss} && eslint --ext .js,.jsx,.ts,.tsx ./client ./server",
    "lint:fix": "stylelint ./client/**/*.{css,scss} --fix && eslint --ext .js,.jsx,.ts,.tsx ./client ./server --fix",
    "server": "nodemon ./server/index.js",
    "start": "concurrently \"pnpm:server\" \"pnpm:client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/hust-oom.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "chat",
    "react",
    "webpack"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/hust-oom/issues"
  },
  "homepage": "https://github.com/sabertazimi/hust-oom",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-object-rest-spread": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@dg-scripts/eslint-config": "^5.14.2",
    "@dg-scripts/stylelint-config": "^5.14.2",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-webpack-plugin": "^4.0.1",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.1",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.1",
    "stylelint-webpack-plugin": "^5.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "dependencies": {
    "@livechat/ui-kit": "^0.5.0-28",
    "body-parser": "^1.20.2",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.2",
    "redux": "^5.0.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4"
  }
}
