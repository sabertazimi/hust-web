{
  "name": "chat",
  "version": "2.9.0",
  "description": "Chat System",
  "author": "sabertazimi",
  "license": "MIT",
  "homepage": "https://github.com/sabertazimi/hust-oom",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/hust-oom.git"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/hust-oom/issues"
  },
  "keywords": [
    "chat",
    "react",
    "webpack"
  ],
  "main": "./client/index.js",
  "engines": {
    "node": ">=9"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "client": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "lint": "stylelint ./client/**/*.{css,scss} && eslint ./client ./server",
    "lint:fix": "stylelint ./client/**/*.{css,scss} --fix && eslint ./client ./server --fix",
    "server": "nodemon ./server/index.js",
    "start": "concurrently \"pnpm:server\" \"pnpm:client\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@livechat/ui-kit": "^0.5.0-29",
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "redux": "^5.0.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-transform-object-rest-spread": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@dg-scripts/eslint-config": "^5.21.1",
    "@dg-scripts/stylelint-config": "^5.21.1",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-webpack-plugin": "^4.2.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.2",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "sass": "^1.77.4",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "stylelint": "^16.6.1",
    "stylelint-webpack-plugin": "^5.0.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
