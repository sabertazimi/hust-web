{
  "name": "chat",
  "version": "2.7.0",
  "description": "Chat System",
  "main": "./client/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "client": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "lint": "stylelint ./client/**/*.{css,scss} && eslint --ext .js,.jsx,.ts,.tsx ./client ./server",
    "lint:fix": "stylelint ./client/**/*.{css,scss} --fix && eslint --ext .js,.jsx,.ts,.tsx ./client ./server --fix",
    "server": "nodemon ./server/index.js",
    "start": "concurrently \"yarn:server\" \"yarn:client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/hust-oom.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "chat",
    "react",
    "webpack"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/hust-oom/issues"
  },
  "homepage": "https://github.com/sabertazimi/hust-oom",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@dg-scripts/eslint-config": "^5.8.0",
    "@dg-scripts/stylelint-config": "^5.8.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.12.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.1",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.1",
    "stylelint-webpack-plugin": "^3.2.0",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@livechat/ui-kit": "^0.5.0-20",
    "body-parser": "^1.19.2",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1"
  }
}
