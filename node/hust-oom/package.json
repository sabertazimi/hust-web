{
  "name": "chat",
  "version": "2.9.0",
  "description": "Chat System",
  "author": "sabertazimi",
  "license": "MIT",
  "homepage": "https://github.com/sabertazimi/hust-oom",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/hust-oom.git"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/hust-oom/issues"
  },
  "keywords": [
    "chat",
    "react",
    "webpack"
  ],
  "main": "./client/index.js",
  "engines": {
    "node": ">=9"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "client": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "lint": "stylelint ./client/**/*.{css,scss} && eslint ./client ./server",
    "lint:fix": "stylelint ./client/**/*.{css,scss} --fix && eslint ./client ./server --fix",
    "server": "nodemon ./server/index.js",
    "start": "concurrently \"pnpm:server\" \"pnpm:client\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@livechat/ui-kit": "^0.5.0-32",
    "body-parser": "^1.20.3",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.0.2",
    "redux": "^5.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@dg-scripts/eslint-config": "^5.21.5",
    "@dg-scripts/stylelint-config": "^5.21.5",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.17.0",
    "eslint-webpack-plugin": "^4.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "stylelint": "^16.11.0",
    "stylelint-webpack-plugin": "^5.0.1",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  }
}
