    "express": "^4.17.2",
    "express-rate-limit": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0"
