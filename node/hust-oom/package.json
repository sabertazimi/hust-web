{
  "name": "chat",
  "version": "2.6.0",
  "description": "Chat System",
  "main": "./client/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "client": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "lint": "stylelint ./client/**/*.scss && eslint --ext .js --ext .jsx ./client ./server",
    "server": "nodemon ./server/index.js",
    "start": "concurrently \"yarn:server\" \"yarn:client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/hust-oom.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "chat",
    "react",
    "webpack"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/hust-oom/issues"
  },
  "homepage": "https://github.com/sabertazimi/hust-oom",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@dg-scripts/eslint-config": "^5.5.10",
    "@dg-scripts/stylelint-config": "^5.5.10",
    "autoprefixer": "^10.4.1",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "sass": "^1.45.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
