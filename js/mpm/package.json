{
  "name": "mpmjs",
  "version": "2.5.0",
  "description": "Minimal package manager",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js ./src",
    "start": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/mpm.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "npm",
    "yarn",
    "package manager"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/mpm/issues"
  },
  "homepage": "https://github.com/sabertazimi/mpm#readme",
  "bin": {
    "mpm": "./src/index.js"
  },
  "files": [
    "src/**/*.js",
    "!src/**/*.test.js"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/*.{js}",
      "!**/index.js",
      "!**/utils.js"
    ]
  },
  "packageManager": "yarn@3.0.2",
  "dependencies": {
    "commander": "^8.2.0",
    "fs-extra": "^10.0.0",
    "gunzip-maybe": "^1.4.2",
    "log4js": "^6.3.0",
    "node-fetch": "^2.6.5",
    "ora": "^5.4.1",
    "semver": "^7.3.5",
    "tar-fs": "^2.1.1",
    "tar-stream": "^2.2.0"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^1.1.2",
    "eslint": "^7.32.0",
    "jest": "^27.2.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  }
}
